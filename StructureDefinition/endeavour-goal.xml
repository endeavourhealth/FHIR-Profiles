  <url value="http://endeavourhealth.org/fhir/StructureDefinition/endeavour-goal" />
  <name value="Endeavour-Goal" />
  <display value="Endeavour goal" />
  <status value="draft" />
  <publisher value="Endeavour Health Charitable Trust" />
  <date value="2015-09-22T20:02:49+10:00" />
  <fhirVersion value="1.0.1" />
  <kind value="resource" />
  <constrainedType value="Goal" />
