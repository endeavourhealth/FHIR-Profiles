      </type>
      <type>
        <code value="Range" />
      </type>
      <type>
        <code value="date" />
      </type>
      <mapping>
        <identity value="rim" />
        <map value="player[classCode=LIV, determinerCode=INSTANCE].deceasedInd, deceasedDate (could be URG)  For age, you'd hang an observation off the role" />
